{
  "name": "@ehpr/api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "echo \"Building @ehpr/api\" && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "nest start --exec \"node -r ./env.js\"",
    "start:dev:local": "nest start --preserveWatchOutput --watch --exec \"node -r ./env.js\"",
    "start:debug:local": "nest start --debug --watch --exec \"node -r ./env.js\"",
    "lint": "echo 'Linting @ehpr/api...' && eslint --fix --cache \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli.js --config src/ormconfig.ts",
    "export-submission": "NODE_ENV=script node -r ts-node/register -r tsconfig-paths/register src/cron.ts"
  },
  "dependencies": {
    "@ehpr/common": "1.0.1",
    "@nestjs/common": "10.3.9",
    "@nestjs/core": "10.3.9",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/platform-express": "10.3.9",
    "@nestjs/swagger": "7.3.1",
    "@nestjs/typeorm": "10.0.2",
    "@supercharge/promise-pool": "3.1.1",
    "@vendia/serverless-express": "4.3.11",
    "aws-sdk": "2.1017.0",
    "axios": "0.24.0",
    "class-transformer": "0.5.1",
    "csv-stringify": "6.0.4",
    "csv-writer": "1.6.0",
    "dayjs": "1.11.9",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "handlebars": "4.7.7",
    "js-yaml": "4.1.0",
    "jsonwebtoken": "9.0.1",
    "jwks-rsa": "3.0.1",
    "limiter": "2.1.0",
    "nanoid": "3.1.30",
    "nest-winston": "1.6.2",
    "nodemailer": "6.9.4",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "secure-random-string": "1.1.4",
    "swagger-ui-express": "4.2.0",
    "typeorm": "0.3.20",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.9",
    "@types/aws-lambda": "8.10.84",
    "@types/express": "4.17.13",
    "@types/js-yaml": "4.0.5",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "16.0.0",
    "@types/nodemailer": "6.4.8",
    "@types/secure-random-string": "1.1.3",
    "@types/supertest": "2.0.11",
    "eslint": "8.57.0",
    "jest": "29.7.0",
    "source-map-support": "0.5.20",
    "supertest": "7.0.0",
    "ts-jest": "29.1.2",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".\\**\\.spec\\.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
