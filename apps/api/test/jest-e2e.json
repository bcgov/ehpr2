{
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "^src(.*)$": "<rootDir>/../src/$1"
  },
  "reporters": ["default"],
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./test-results/coverage",
  "testEnvironment": "node"
}
